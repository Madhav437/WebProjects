<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <!--write code here-->
 <class dynamic-insert="false" dynamic-update="false" mutable = "true" name="edu.neu.hwmvc.model.Customer" optimistic-lock="version"
 polymorphism="implicit" select-before-update="false" table="Customer">
 
 <id name="custId" type="integer">
   <column name="custId"/>
